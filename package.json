    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.49.0",
    "babel-jest": "^29.0.1",
    "babel-plugin-tsconfig-paths-module-resolver": "^1.0.3",
    "eslint-config-standard-with-typescript": "^32.0.0",
