  "dependencies": {
    
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.0",
